        <dependency id="MarkdownDeep.NET" version="1.5" />
        <dependency id="YamlDotNet" version="3.7.0" />
        <dependency id="Shortcoder" version="0.2.0" />
        <dependency id="Microsoft.AspNet.Mvc" version="5.2.3" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="..\Encel\bin\Release\Encel.dll" target="lib\net45\" />
  </files>
</package>